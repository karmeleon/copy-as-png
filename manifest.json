{
	"manifest_version": 2,
	"name": "Copy as PNG",
	"version": "1.0.2",
	"description": "Copy any image on the 'net as PNG",
	"permissions": ["menus", "clipboardWrite", "scripting", "<all_urls>"],
	"background": {
		"scripts": ["main.js"],
		"persistent": false
	},
	"icons": {
		"48": "res/png-icon-light.svg",
		"96": "res/png-icon-light.svg"
	}
}
